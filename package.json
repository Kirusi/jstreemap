{
  "name": "setmap",
  "version": "1.7.1",
  "description": "Library of associative containers.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "eslint": "eslint --config=./build/.eslintrc.json **/*.js",
    "eslint-watch": "cross-env node_modules/eslint-watch/bin/esw --config=./build/.eslintrc.json -w --fix **/*.js",
    "test-watch": "cross-env node_modules/.bin/mocha --reporter spec -w test/**/*.js",
    "test": "cross-env SETTINGS_FILE=settings.yml nyc --reporter=lcov --reporter=html --reporter=json --reporter=text --report-dir=.coverage node_modules/.bin/mocha --reporter spec --recursive test/**/*.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirusi/setmap.git"
  },
  "keywords": [
    "set",
    "map",
    "red",
    "black",
    "tree"
  ],
  "author": "Kirusi Msafiri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kirusi/setmap/issues"
  },
  "homepage": "https://github.com/kirusi/setmap#readme",
  "dependencies": {
    "eslint-watch": "^3.1.3"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "mocha": "^4.0.1"
  }
}
