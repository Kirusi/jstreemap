    "red",
    "black",
    "tree"
  ],
  "author": "Kirusi Msafiri",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kirusi/setmap/issues"
  },
  "homepage": "https://github.com/kirusi/setmap#readme",
  "dependencies": {
    "app-module-path": "^2.2.0",
    "should": "^13.1.3"
  },
  "devDependencies": {
    "cross-env": "^5.1.1",
    "eslint": "^4.13.1",
    "eslint-watch": "^3.1.3",
    "mocha": "^4.0.1",
    "mocha-webpack": "^1.0.1",
    "nyc": "^11.4.1",
    "webpack": "^3.10.0"
  }
